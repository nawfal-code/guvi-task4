package guvi.task4;

import java.util.Stack;

public class StackSol {
    public static void main(String[] args) {
        Stack<Integer> st=new Stack<>();

        //pushing elemnets on to the stack
        st.push(11);
        st.push(22);
        st.push(33);
        st.push(44);
        st.push(55);

        //popping(removes) elements from the stack(from last to first)
      for (int i=0;i<5;i++) {
          System.out.println(st.pop());
      }
      
      //After popping,checking if the Stack is empty or not
            if(st.empty()){
                System.out.println("Stack is empty");
            }
            else {
                System.out.println("Stack has elements");
            }
    }
}
